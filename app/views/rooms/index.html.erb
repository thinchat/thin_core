<h1>Room list</h1>
<%= link_to "Create a room", rooms_path, :method => :post %>
<ul>
<% @rooms.each do |room| %>
  <li><%= link_to room.name, room_path(room) %></li>
<% end %>
</ul>

<% unless @user.class == 'Guest' %>
  <%= "#{@user.inspect}" %>
<% end %>

<script type="text/javascript">
  faye_url = '<%= FAYE_URL %>'
  user = '<%= current_user.user_json.html_safe %>'
</script>